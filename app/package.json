{
  "name": "fks-landing-frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "node server.js & node scripts/inject-password-hash.js && vite",
    "dev:fast": "node server.js & vite",
    "dev:vite": "node scripts/inject-password-hash.js && vite",
    "dev:server": "node server.js",
    "dev:vercel": "vercel dev",
    "build": "node scripts/inject-password-hash.js && tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "vercel:dev": "vercel dev",
    "vercel:deploy": "vercel --prod",
    "stripe:listen": "stripe listen --forward-to localhost:3000/api/webhooks/stripe",
    "stripe:trigger:checkout": "stripe trigger checkout.session.completed",
    "stripe:trigger:payment": "stripe trigger payment_intent.succeeded",
    "stripe:trigger:subscription": "stripe trigger customer.subscription.created"
  },
  "engines": {
    "node": "22.x"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@stripe/stripe-js": "^8.5.3",
    "@vercel/node": "^4.0.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "framer-motion": "^10.16.16",
    "lucide-react": "^0.303.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.49.2",
    "react-router-dom": "^6.21.1",
    "resend": "^6.5.2",
    "stripe": "^20.0.0",
    "tailwind-merge": "^2.2.0",
    "zod": "^3.22.4",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "terser": "^5.44.1",
    "typescript": "^5.2.2",
    "vite": "^7.3.1"
  }
}
